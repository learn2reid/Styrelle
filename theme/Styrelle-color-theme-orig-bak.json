{
	"name": "Styrelle",
	"type": "dark",
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				// "foreground": "#5a5953"
				"foreground": "#4A4943"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#CCCC97"
				// "foreground": "#e0e096"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#e00606"
				// "foreground": "#e04b06"  // Orange
				// "foreground": "#9D4CFF" // Purple
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#9D4CFF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#a567f1"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#FA236E",
				"fontStyle": " bold"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#FA236E"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic ",
				"foreground": "#3BE5FF"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#89e215",
				"fontStyle": " italic "
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#89e215",
				"fontStyle": " italic"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#89e215"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic ",
				"foreground": "#FFA826"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#F92672"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#89e215"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"foreground": "#3BE5FF"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#3BE5FF"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF0D31"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "JSON - String",
			"scope": "source.json.comments",
			"settings": {
				"fontStyle": "",
				"foreground": "#3BE5FF"
			}
		}
	],
	"colors": {
		"activityBar.background": "#000000",
		"activityBar.foreground": "#ffffff",
		// "activityBarBadge.background": "#80CBC4",
		// "activityBarBadge.background": "#222222",
		"activityBarBadge.background": "#218ee0",
		"activityBarBadge.foreground": "#ffffff",
		"badge.background": "#f1f1f1",
		"badge.foreground": "#2b21b4",
		"button.background": "#2b21b4",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#2b21b4be",
		"descriptionForeground": "#8787a7",

		// Dropdown
		"dropdown.background": "#010101",
		"dropdown.border": "#333333",
		"dropdown.foreground": "#ffffff",
		"dropdown.listBackground": "#000000",

		// Editor
		"editor.background": "#000000",
		"editor.foreground": "#FFFFFF",
		// "editor.lineHighlightBackground": "#333333",
		"editor.lineHighlightBackground": "#0f0f0f",
		"editor.lineHighlightBorder": "#1f1f1f",
		"editor.selectionBackground": "#2b21b4",
		"editorCursor.foreground": "#FFFFFF",
		"editorGroupHeader.noTabsBackground": "#000000",
		"editorGroupHeader.tabsBackground": "#000000",

		// Gutter
		"editorGutter.background": "#000000",
		"editorGutter.foldingControlForeground": "#FF00FF",

		"editorIndentGuide.activeBackground": "#ffffff",
		// "editorIndentGuide.background": "#777777",
		"editorIndentGuide.background": "#000",
		"editorRuler.foreground": "#777777",
		"editorWhitespace.foreground": "#5C5C58",
		"focusBorder": "#000000",
		"foreground": "#ffffff",

		// Git
		"gitDecoration.addedResourceForeground": "#89e215",
		"gitDecoration.conflictingResourceForeground": "#f0af2e",
		"gitDecoration.deletedResourceForeground": "#cf3838",
		"gitDecoration.ignoredResourceForeground": "#e0866b",
		"gitDecoration.modifiedResourceForeground": "#CCCC97",
		"gitDecoration.submoduleResourceForeground": "#cf1db2",
		"gitDecoration.untrackedResourceForeground": "#3BE5FF",

		// Input
		"input.background": "#101010",
		"input.border": "#fafafa",

		// List
		"list.activeSelectionBackground": "#111132",
		"list.activeSelectionForeground": "#e4e4e4",
		"list.dropBackground": "#000000",
		"list.focusBackground": "#222222",
		"list.focusForeground": "#e4e4e4",
		"list.highlightForeground": "#495288",
		"list.hoverBackground": "#222222",
		"list.hoverForeground": "#e4e4e4",
		"list.inactiveFocusBackground": "#222222",
		"list.inactiveSelectionBackground": "#111132",
		"list.inactiveSelectionForeground": "#e4e4e4",
		"list.invalidItemForeground": "#e4e4e4",

		"selection.background": "#53717a",

		// Sidebar
		"sideBarSectionHeader.background": "#080808",
		"sideBar.background": "#000000",
		"sideBar.foreground": "#fafafa",
		"sideBarTitle.foreground": "#fafafa",

		// Status Bar
		"statusBar.background": "#000000",
		"statusBar.noFolderBackground": "#000000",
		"statusBar.debuggingBackground": "#462020",
		"statusBar.foreground": "#ffffff",

		// Tab
		"tab.activeBackground": "#000000",
		"tab.activeBorder": "#cccccc",
		"tab.activeForeground": "#fafafa",
		"tab.border": "#000000",
		"tab.inactiveBackground": "#000000",
		"tab.inactiveForeground": "#fafafa",

		// Title Bar
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#fafafa",
		"titleBar.border": "#000000",
		// "titleBar.inactiveBackground": "#0f0f0f",
		"titleBar.inactiveBackground": "#222222",
		"titleBar.inactiveForeground": "#aaaaaa",

		// Suggest Widget
		"editorSuggestWidget.background": "#000000",
		"editorSuggestWidget.selectedBackground": "#1b1b1b",
		"editorSuggestWidget.border": "#1b1b1b",
		"editorSuggestWidget.foreground": "#cccccc",

		// "editorSuggestWidget.highlightForeground": "#ffffff",
		"editorSuggestWidget.highlightForeground": "#ff4040",
		"editorWidget.background": "#000000",
		"editorWidget.border": "#1b1b1b",
		"editorWidget.resizeBorder": "#1b1b1b",

		// Hover Widget
		"editorHoverWidget.background": "#0a0a0a",
		"editorHoverWidget.border": "#1b1b1b",
		"debugToolBar.background": "#000000",
		"debugToolBar.border": "#1b1b1b",

		// Errors
		// "list.errorForeground": "#fad325",
		"list.errorForeground": "#ff4040",
		"editorError.foreground": "#ff4040",

		// Warnings
		"list.warningForeground": "#ff9100",
		// "list.warningForeground": "#ff4040",
		"editorWarning.foreground": "#ff9100",
		// Infos
		"editorInfo.foreground": "#ffee00",
	}
}
